/* pages/mine/mine.wxss */
.personal_all{
  width: 100%;
  height: 400rpx;
  background-color: #D8D8D8;
}
.setup{
  display: flex;
  float: left;
  width: 50rpx;
  height: 50rpx;
  margin-left: 4%;
}
.personal{
  width: 20%;
  float: right;
  margin-right:40% 
}
.button{
  display: flex;
  width: 29%;
  float: right;
  margin-right: 14%;
  margin-top: 100rpx;
  align-items: center;
  justify-content: center; /*居中属性*/
}
.myorders{
  display: flex;
  width: 100%;
  height: 80rpx;
  align-items: center;
}
.orders{
  float: left;
  align-content:left;
  margin-left: 2%;
}
.allorders{
  align-content: right;
  margin-left: 40%;
  color: #D8D8D8;
}
.cut-off{
  border: 0;
  border-bottom: 1px solid #ccc;
  background: #999;
}
.type{
  display:flex;
  flex-direction:row;
}
.types{
 width: 15%;
 height: 112.5rpx;
 margin-top: 10rpx;
 margin-left: 8%;
 float: left;/*这里不能使用display：inline 因为使用该属性会导致宽度值和高度值 不起作用*/
 font-size: 26rpx;
 text-align: center;
}
.image_list{
  width: 100%;
  height:100%;
  border: 0 solid #ff0000;
  border-radius: 100rpx;
  background-color: #f10b2e;
}
.wcut-off{
  display:flex;
  height: 20rpx;
  width: 100%;
  background-color: #D8D8D8;
  margin-top: 40rpx;
}
.mymodule{
  display:flex;
  width: 100%;
  height: 100rpx;
  align-items: center;
  margin-left: 2%;
}
.mymodule-img{
  width: 50rpx;
  height: 50rpx;
}
.mymodule-title{
  width: 100%;
  margin-left: 50rpx;
  color: #D8D8D8;
}
.superscript{
  float: right;
  margin-right: 5%;
}