page{  
background: #f5f5f5;
} 
.search{
  display: flex;
  flex-direction: row;
}
.search-border{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color:#ff1;
  border-style: solid;
  border-color: #dedede;
}
.classify-search{
  width: 50rpx;
  height: 50rpx;
}.search-input{
  height: 80rpx;
  width: 90%;
}
/*总体主盒子*/  
.container {  
position: relative;  
width: 100%;  
height: 1000rpx;  
background-color: #fff;  
color: #939393;  
}
/*左侧栏主盒子*/  
.nav_left{  
 /*左侧盒子使用了绝对定位*/  
position: absolute;  
top: 0;  
left: 0;  
display: inline-block;  
width: 25%;  
height: 100%;  
/*主盒子设置背景色为灰色*/  
background: #f5f5f5;  
text-align: center;  
}  
/*左侧栏list的item*/  
.nav_left .nav_left_items{  
/*每个高30px*/  
height: 80rpx;  
/*垂直居中*/  
line-height: 80rpx;  
/*再设上下padding增加高度，总高42px*/  
padding: 12rpx 0;  
/*只设下边线*/  
border-bottom: 2rpx solid #dedede;  
background-color: #f5f5f5;
/*文字14px*/  
font-size: 28rpx; 
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
/*背景色变成白色*/  
background: #fff;  
color: #f0145a; 
}  
/*右侧栏主盒子*/  
.nav_right{  
/*右侧盒子使用了绝对定位*/  
position: absolute;  
top: 0;  
right: 0;  
flex: 1;  
/*宽度75%，高度占满，并使用百分比布局*/  
width: 75%;  
height: 1000rpx;  
padding: 20rpx;  
box-sizing: border-box;  
background: #fff;  
}  
/*右侧栏list的item*/  
.nav_right .nav_right_items{  
/*浮动向左*/  
float: left;  
/*每个item设置宽度是33.33%*/  
width: 33.33%;  
height: 240rpx;  
text-align: center;  
}  
.nav_right .nav_right_items image{  
/*被图片设置宽高*/  
width: 120rpx;  
height: 120rpx;  
margin-top: 30rpx;  
}  
.nav_right .nav_right_items text{  
/*给text设成块级元素*/  
display: block;  
margin-top: 30rpx;  
font-size: 28rpx;  
color: black;
/*设置文字溢出部分为...*/  
overflow: hidden;  
white-space: nowrap;  
text-overflow: ellipsis;  
} 
.nodata_text
{
color: black;
font-size: 28rpx;  
text-align: center;  
}